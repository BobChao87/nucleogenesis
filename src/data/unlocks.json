{
  "exotic_gate": {
    "condition": ["(() => {",
      "return state.player.statistics.exotic_run.H &&",
      "       state.player.statistics.exotic_run.H['1H'] &&",
      "       state.player.statistics.exotic_run.H['1H'] >= 1e8",
      "})()"
    ]
  },
  "isotope": {
    "condition": ["(() => {",
      "for(let key in player.resources){",
      "  if (data.resources[key].type.indexOf('isotope') !== -1 &&",
      "      key !== '1H' &&",
      "      player.resources[key].unlocked) {",
      "    return true;",
      "  }",
      "}",
      "})()"
    ]
  },
  "molecule": {
    "condition": ["(() => {",
      "for(let key in player.resources){",
      "  if (data.resources[key].type.indexOf('molecule') !== -1 &&",
      "      player.resources[key].unlocked) {",
      "    return true;",
      "  }",
      "}",
      "})()"
    ]
  },
  "ion": {
    "condition": ["(() => {",
      "for(let key in player.resources){",
      "  if (data.resources[key].type.indexOf('ion') !== -1 &&",
      "      player.resources[key].unlocked) {",
      "    return true;",
      "  }",
      "}",
      "})()"
    ]
  },
  "dark_gate": {
    "condition": ["(() => {",
      "return state.player.statistics.dark_run.xH &&",
      "       state.player.statistics.dark_run.xH >= 1e3",
      "})()"
    ]
  },
  "fusion_gate": {
    "condition": ["(() => {",
      "return state.player.statistics.all_time.eV &&",
      "       state.player.statistics.all_time.eV >= 1e30",
      "})()"
    ]
  },
  "table_gate": {
    "condition": "player.resources['dark_matter'].unlocked"
  },
  "radioactive": {
    "condition": ["(() => {",
      "for(let key of data.radioisotopes){",
      "  if (player.resources[key].unlocked) {",
      "    return true;",
      "  }",
      "}",
      "return false;",
      "})()"
    ]
  },
  "misc": {
    "condition": ["(() => {",
      "for(let key in player.resources){",
      "  if (data.resources[key].type.indexOf('isotope') === -1 &&",
      "      data.resources[key].type.indexOf('ion') === -1 &&",
      "      data.resources[key].type.indexOf('molecule') === -1 &&",
      "      player.resources[key].unlocked) {",
      "    return true;",
      "  }",
      "}",
      "})()"
    ]
  }
}
